{
    "name": "modules/my-file",
    "description": "My File module for Laravel application with media library integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0|^12.0",
        "spatie/laravel-medialibrary": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\MyFile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\MyFile\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\MyFile\\Providers\\MyFileServiceProvider"
            ]
        }
    }
}